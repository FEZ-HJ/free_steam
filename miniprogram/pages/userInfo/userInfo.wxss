.calendar {
    background-color: #eeedff;
    padding-top: 10px;
}

.header {
    font-size: large; /*修改了标题文字和按钮的大小*/
    color: #605eac;   /*修改了标题文字和按钮的颜色*/
}

.constant{
  position:fixed;
  z-index: 99;
  height: 100rpx;
  width: 100%;
}

.intro {
  margin: 30px;
  text-align: center;
}
.intro-title {
  font-size: 24px;
  font-weight: 400;
  color: #333;
}
.intro-url {
  font-size: 16px;
  font-weight: 300;
  color: #666;
  margin-top: 10px;
}
.mybutton {
  color: #333;
  /* background-color: #EEE; */
  border: 5px solid rgba(255, 255, 255, 0.7);
  background-color:rgba(238, 238, 238, 0.8);
  width: 100px;
  height: 100px;
  border-radius: 100px;
  display: flex;
  align-items: center;
  justify-content: center;
}
.body {
  padding: 20px;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  /* position:fixed; */
  /* left:350rpx; */
  /* top:750rpx; */
}
.cell {
  margin-top: 10px;
  border-top: 1px solid #EEE;
  padding-top: 10px;
  font-size: 18px;
  color: #333;
}
.cell > text {
  margin-right: 20px;
}
.tips, .warn {
  padding: 5px 10px;
  margin: 10px;
  border-radius: 5px;
  text-align: center;
  background-color: rgba(3, 169, 244, 0.1);
}
.tips > text, .warn > text {
  font-size: 12px;
  color: #03a9f4;
  font-weight: 200;
}
.warn {
  background-color: rgba(255, 152, 0, 0.1);
}
.warn > text {
  color: #ff9800;
}

.tab{
  height: 100rpx;
  width: 100%;
  display:flex;
  flex-direction:row;
  justify-content:space-between;
}

.left{
  vertical-align:middle;
  display:flex;
  flex-direction:row;
  justify-content:center;
  margin-top: 20rpx;
}

.leftImage{
  margin-top: 13rpx;
  width:35rpx;
  height:35rpx;
  margin-left: 35rpx
}

.text{
  margin-left: 10rpx;
  margin-top: 10rpx;
  font-size:30rpx;
}
